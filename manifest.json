{
    "name": "Blocker",
    "description": "Blocker",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "webRequest",
        "browsingData",
        "storage",
        "declarativeNetRequest",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "src/background.js"
    },
    "action": {
        "default_popup": "src/popup/popup.html"
    }
}